{
    "name": "jameshalsall/licenser",
    "description": "Adds license headers to your source files",
    "require": {
        "symfony/console": "~2.8|~3.0",
        "symfony/finder": "~2.8|~3.0",
        "pimple/pimple": "~3.0",
        "twig/twig": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6|~5.0"
    },
    "autoload": {
        "psr-4": {
            "JamesHalsall\\Licenser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JamesHalsall\\Licenser\\Tests\\": "tests/Licenser"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Halsall",
            "email": "james.t.halsall@googlemail.com"
        }
    ],
    "bin": [
        "bin/licenser"
    ]
}
