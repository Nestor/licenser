    "require": {
        "symfony/console": "2.4.x-dev",
        "symfony/finder": "2.4.x-dev",
        "pimple/pimple": "1.1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "dev-master"
    },
    "autoload": {
